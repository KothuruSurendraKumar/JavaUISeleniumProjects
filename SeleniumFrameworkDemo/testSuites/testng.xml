<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Suite">

<listeners>
	<listener class-name="SeleniumJavaFramework.testComponents.TestNGListeners"/>
</listeners>

  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="SeleniumJavaFramework.tests.StandaloneUsingFramework"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="SeleniumJavaFramework.tests.ErrorHandilingTests"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
